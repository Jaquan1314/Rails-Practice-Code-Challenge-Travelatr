<h1>Blogger Form</h1>

<%= form_for(@blogger) do |f| %>
    <p>
        <%= f.label :name %>
        <%= f.text_field :name %>
    </p>
    <p>
        <%= f.label :bio %>
        <%= f.text_field :bio %>
    </p>
    <p>
        <%= f.label :age %>
        <%= f.text_field :age %>
    </p>

    <p>
        <%= f.submit %>
    </p>
<% end %>